<h1>Messages</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Message</th>
        <th>E-mail</th>
    </tr>
    </thead>
    <tbody>
    {{#each model as |invitation|}}
        <tr>
            <th>{{invitation.message}}</th>
            <td>{{invitation.email}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>